{
    "name": "auto-tag-selector",
    "version": "1.0.0",
    "description": "React component to item select in a collection with autocomplete feature. It has semantic-ui-react dependency...",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "build": "tsc",
        "format": "prettier --write \"src/**/*.tsx\"",
        "lint": "tslint -p tsconfig.json",
        "prepare": "npm run build",
        "prepublishOnly": "npm run lint",
        "preversion": "npm run lint",
        "version": "npm run format && git add -A src",
        "postversion": "git push && git push --tags"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nquis/auto-tag-selector.git"
    },
    "keywords": [
        "react",
        "semantic-ui-react",
        "input",
        "collection",
        "autocomplete",
        "tag",
        "tag-input"
    ],
    "author": "Fikret OZTURK - nQuis Technology",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/nquis/auto-tag-selector/issues"
    },
    "homepage": "https://github.com/nquis/auto-tag-selector#readme",
    "devDependencies": {
        "@types/lodash": "^4.14.162",
        "@types/react": "^16.9.52",
        "lodash": "^4.17.20",
        "prettier": "^2.1.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "semantic-ui-react": "^2.0.0",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^4.0.3"
    },
    "peerDependencies": {
        "@types/lodash": "^4.14.162",
        "@types/react": "^16.9.52",
        "lodash": "^4.17.20",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "semantic-ui-react": "^2.0.0"
    },
    "files": [
        "lib/**/*"
    ]
